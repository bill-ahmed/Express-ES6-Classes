{
  "name": "eec",
  "version": "1.1.1",
  "description": "Define ExpressJS routes as Typescript classes.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "express",
    "expressjs",
    "MVC",
    "controllers",
    "es6 classes"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node-dev --respawn --transpile-only src/examples/index.ts",
    "test": "ava --verbose",
    "build": "tsc",
    "buildClean": "rm -rf lib && tsc",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bill-ahmed/eec.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bill-ahmed/eec/issues"
  },
  "homepage": "https://github.com/bill-ahmed/eec#readme",
  "dependencies": {
    "express": "^4.17.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "ava": "^3.14.0",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
